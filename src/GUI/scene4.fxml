<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" styleClass="label-Opening" stylesheets="@rawan.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Scene4Controller">
    <children>
        <Label layoutX="205.0" layoutY="43.0" prefHeight="58.0" prefWidth="325.0" styleClass="label-Opening" text="Manage Appointments" />
        <Button fx:id="viewAppointmentsButton" layoutX="510.0" layoutY="180.0" mnemonicParsing="false" onAction="#handleViewAppointments" prefHeight="58.0" prefWidth="222.0" styleClass="button-Manage" text="Schedule Appointments" />
        <Button fx:id="addAppointmentButton" layoutX="510.0" layoutY="271.0" mnemonicParsing="false" onAction="#handleAddAppointment" prefHeight="58.0" prefWidth="222.0" styleClass="button-Manage" text="Reschedule Appointment" />
        <Button fx:id="cancelAppointmentButton" layoutX="510.0" layoutY="363.0" mnemonicParsing="false" onAction="#handleCancelAppointment" prefHeight="58.0" prefWidth="222.0" styleClass="button-Manage" text="Cancel Appointment" />

        <!-- Back Button -->
        <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleBackToPreviousScene" prefHeight="40.0" prefWidth="67.0" styleClass="button-Login" stylesheets="@rawan.css" text="Back">
            <font>
                <Font size="16.0" />
            </font>
        </Button>

        <Pane layoutX="96.0" layoutY="191.0" prefHeight="260.0" prefWidth="246.0" styleClass="anchor-pane3" />

        <!-- Add To Waiting List Button -->
        <Button fx:id="addToWaitingListButton" layoutX="510.0" layoutY="451.0" mnemonicParsing="false" onAction="#handleAddToWaitingList" prefHeight="58.0" prefWidth="222.0" styleClass="button-Manage" text="Add To WaitingList"/>
    </children>
</AnchorPane>
