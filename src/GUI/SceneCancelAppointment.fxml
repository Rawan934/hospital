<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="label-Opening" stylesheets="@rawan.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.SceneCancelAppointmentController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" prefHeight="26.0" prefWidth="194.0" text="Cancel Appointment">
            <font>
                <Font name="Segoe UI Black" size="16.0" />
            </font>
        </Label>
        <TextField fx:id="appointmentId" layoutX="310.0" layoutY="151.0" prefHeight="14.0" prefWidth="213.0" promptText="Appointment Id">
            <font>
                <Font size="14.0" />
            </font>
        </TextField>
        <Button fx:id="cancelButton" layoutX="376.0" layoutY="201.0" mnemonicParsing="false" onAction="#handleCancelAppointment" prefHeight="38.0" prefWidth="81.0" text="Cancel">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Label fx:id="resultLabel" layoutX="260.0" layoutY="250.0" prefHeight="20.0" prefWidth="295.0" text="">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <!-- Back Button -->
        <Button layoutX="14.0" layoutY="350.0" mnemonicParsing="false" onAction="#handleBack" prefHeight="30.0" prefWidth="48.0" styleClass="button-Login" text="Back">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
      <Pane layoutX="38.0" layoutY="90.0" prefHeight="200.0" prefWidth="200.0" styleClass="anchor-pane9" />
    </children>
</AnchorPane>
